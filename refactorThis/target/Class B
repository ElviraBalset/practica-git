public class B {
/**
 * Calcula el bono de una persona segÃºn su salario y edad
 */
    
    public double calcularBono() {
       
        double bono = 0;
        if (A.getedad() > 50) {
            bono = A.getsalario() * 0.10;
        } else {
            bono = A.getsalario() * 0.05;
          
    }
}
